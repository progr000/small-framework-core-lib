{
    "name": "maksym/small-framework-core-lib",
    "type": "library",
    "description": "Package framework core lib",
    "keywords": ["framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Maksym Haivoronskyi",
            "email": "maksym.haivoronskyi@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-json": "*",
	    "ext-mbstring": "*",
        "ext-fileinfo": "*",
        "ext-pdo": "*",
        "ext-iconv": "*",
        "ext-openssl": "*",
        "ext-curl": "*",
        "ext-memcached": "*"
    },
    "autoload": {
        "files": [
            "src/functions/dumper.php",
            "src/functions/helpers.php"
        ],
        "psr-4": {
            "Core\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
